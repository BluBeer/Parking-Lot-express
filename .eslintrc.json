{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":["error", "windows"],
        "no-console":"off",
        "prefer-destructuring":"off",
        "no-plusplus":"off",
        "no-tabs":"off",
        "quote-props":"off",
        "eqeqeq":"off"
    }
}
